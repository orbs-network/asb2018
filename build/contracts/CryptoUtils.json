  "devdoc": {
    "methods": {
      "isMerkleProofValid(bytes32[],bytes32,bytes)": {
        "details": "Verifies the Merkle proof for the existence of a specific data. Please not that that this implementation assumes that each pair of leaves and each pair of pre-images are sorted (see tests for examples of construction).",
        "params": {
          "_leaf": "bytes The data to check.",
          "_proof": "bytes32[] The Merkle proof containing sibling SHA256 hashes on the branch from the leaf to the root.",
          "_root": "bytes32 The Merkle root."
        }
      },
      "isMerkleProofValid(bytes32[],bytes32,bytes32)": {
        "details": "Verifies the Merkle proof for the existence of a specific data. Please not that that this implementation assumes that each pair of leaves and each pair of pre-images are sorted (see tests for examples of construction).",
        "params": {
          "_leafHash": "bytes32 The hash of the data to check.",
          "_proof": "bytes32[] The Merkle proof containing sibling SHA256 hashes on the branch from the leaf to the root.",
          "_root": "bytes32 The Merkle root."
        }
      },
      "isSignatureValid(bytes32,bytes,address)": {
        "details": "Verifies ECDSA signature of a given message hash.",
        "params": {
          "_address": "The public address of the signer (allegedly).",
          "_hash": "bytes32 The hash which is the signed message.",
          "_signature": "bytes The signature to verify."
        }
      },
      "toAddress(bytes)": {
        "details": "Converts a public key to an address.",
        "params": {
          "_publicKey": "bytes32 The uncompressed public key."
        }
      }
    },
    "title": "Cryptographic utilities."
